main: function void () = {
    i: integer;
    j: integer;
    tablas: array integer[10][10];

    for (i = 1; i <= 10; i = i + 1) {
        for (j = 1; j <= 10; j = j + 1) {
            tablas[i-1][j-1] = i * j;
        }
    }

    // Mostrar la tabla del 5
    for (j = 1; j <= 10; j = j + 1) {
        print "5 x ", j, " = ", tablas[4][j-1], "\n";
    }
}
